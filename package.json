{
  "name": "fp-homework",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "classnames": "2.2.6",
    "lodash": "4.17.21",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint:check": "npx eslint .",
    "formatter:check": "npx prettier . --check"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@tsconfig/create-react-app": "^2.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:check"
    ],
    "**/*.{js,jsx,ts,tsx,html,css}": [
      "npm run formatter:check"
    ]
  }
}
